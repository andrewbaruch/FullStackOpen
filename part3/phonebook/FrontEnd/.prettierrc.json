{
  "arrowParens": "always",
  "bracketSameLine": false,
  "objectWrap": "collapse",
  "bracketSpacing": true,
  "semi": false,
  "experimentalOperatorPosition": "start",
  "experimentalTernaries": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "trailingComma": "none",
  "singleAttributePerLine": true,
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "proseWrap": "always",
  "endOfLine": "lf",
  "insertPragma": false,
  "printWidth": 80,
  "requirePragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "embeddedLanguageFormatting": "auto",
  "rangeStart": 0,
  "cursorOffset": 0
}
